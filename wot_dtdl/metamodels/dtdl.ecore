  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ComplexSchema" abstract="true" eSuperTypes="#//Schema"/>
  <eClassifiers xsi:type="ecore:EEnum" name="PrimitiveType">
    <eLiterals name="STRING"/>
    <eLiterals name="BOOLEAN"/>
    <eLiterals name="INTEGER"/>
    <eLiterals name="DOUBLE"/>
    <eLiterals name="LONG"/>
    <eLiterals name="FLOAT"/>
    <eLiterals name="DATE"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Array" eSuperTypes="#//ComplexSchema">
    <eStructuralFeatures xsi:type="ecore:EReference" name="elementSchema" lowerBound="1"
        eType="#//Schema" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Object" eSuperTypes="#//ComplexSchema">
    <eStructuralFeatures xsi:type="ecore:EReference" name="fields" upperBound="-1"
        eType="#//Field" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Field" eSuperTypes="#//Element">
